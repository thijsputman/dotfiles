[wsl2]
processors=16
memory=32GB
dnsTunneling=true
firewall=true
networkingMode=mirrored
swap=0
kernelCommandLine=lsm=apparmor,landlock,lockdown,yama,loadpin,safesetid,integrity,selinux,tomoyo

[experimental]
autoMemoryReclaim=gradual
sparseVhd=true
hostAddressLoopback=true
bestEffortDnsParsing=true
